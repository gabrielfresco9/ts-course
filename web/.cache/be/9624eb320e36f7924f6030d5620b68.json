nal":{"line":25,"column":0},"generated":{"line":66,"column":3}}],"sources":{"src/models/Model.ts":"import {AxiosResponse} from 'axios';\nimport {Callback} from \"./Eventing\";\nimport {Serializable} from \"./ApiSync\";\n\ninterface ModelAttributes<T> {\n    set(value: T): void;\n\n    get<K extends keyof T>(key: K): T[K];\n\n    getAll(): T;\n}\n\ninterface Sync<T> {\n    fetch(id: number): Promise<AxiosResponse<T>>;\n\n    save(data: T): Promise<AxiosResponse<T>>;\n}\n\ninterface Events {\n    on(eventName: string, callback: Callback): void;\n\n    trigger(eventName: string): void;\n}\n\nexport default abstract class Model<T extends Serializable> {\n    constructor(private attributes: ModelAttributes<T>,\n                private events: Events,\n                private sync: Sync<T>) {\n    }\n\n\n    get on() {\n        return this.events.on;\n    }\n\n    get trigger() {\n        return this.events.trigger;\n    }\n\n    get get() {\n        return this.attributes.get;\n    }\n\n    set(update: T): void {\n        this.attributes.set(update);\n        this.events.trigger('change')\n    }\n\n    fetch(): void {\n        const id = this.get('id');\n\n        if (!id) {\n            throw new Error(\"Can't fetch. Id not defined\");\n        } else {\n            this.sync.fetch(id).then((res: AxiosResponse): void => {\n                this.set(res.data);\n            })\n        }\n    }\n\n    save = (): void => {\n        this.sync.save(this.attributes.getAll()).then((res: AxiosResponse): void => {\n            this.trigger('save');\n        });\n    }\n}"},"lineCount":null}},"error":null,"hash":"3a0572116326fafc4b7596d1210c7588","cacheData":{"env":{}}}